<h2>Show video</h2>

<ul>

  <li>
    <strong>User:</strong>
    <%= @video.user_id %>
  </li>

  <li>
    <strong>Category:</strong>
    <%= @video.category.name %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @video.url %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @video.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @video.description %>
  </li>

  <li>
    <strong>View Count:</strong>
    <%= @video.view_count %>
  </li>

</ul>

<%= link "Edit", to: video_path(@conn, :edit, @video), class: "btn btn-default btn-xs" %><span/>
<%= link "Back", to: video_path(@conn, :index) , class: "btn btn-default btn-xs"%>
